  "name": "click-here",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "UNLICENSED",
  "homepage": "https://click-here-dev.herokuapp.com/",
  "main": "src/index.js",
  "dependencies": {
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-scripts": "1.1.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.4.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.4",
    "expect": "^22.4.3",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.7.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "sinon": "^4.5.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "mocha -r jsdom-global/register --require babel-core/register --require ignore-styles --require babel-polyfill --require babel-register --reporter spec setupTests.js \"src/**/*.spec.js\"",
    "coverage": "nyc --reporter=html --reporter=text --reporter=lcov npm test",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "esw src --color",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "eject": "react-scripts eject"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 50,
    "statements": 50,
    "functions": 50,
    "branches": 50,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ],
    "ignore-class-method": "methodToIgnore"
  }
}
